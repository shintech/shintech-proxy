{
  "name": "shintech-proxy",
  "version": "0.2.7",
  "main": "build/server.js",
  "repository": "https://github.com:/shintech/shintech-proxy",
  "license": "MIT",
  "scripts": {
    "start": "node build/server.js",
    "dev": "TARGET=8000 PORT=8443 NODE_ENV=development DOMAIN_NAME=dev.shintech.ninja TZ='America/Chicago' nodemon build/server.js",
    "test": "TZ='America/Chicago' DOMAIN_NAME=dev.shintech.ninja NODE_ENV=test mocha build/test -t 10000",
    "build": "babel -d build src",
    "watch": "babel --watch -d build src | ts '[%Y-%m-%d %H:%M:%S]'"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-latest": "^6.22.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "connect": "^3.6.2",
    "express": "^4.15.3",
    "http-proxy-middleware": "^0.17.4",
    "winston-color": "^1.0.0"
  }
}
